{
  "name": "drips",
  "version": "0.0.1",
  "description": "A Vue.js 2.0 Mobile UI at ShuiDi.",
  "main": "lib/drips.js",
  "style": "lib/drips-css/index.css",
  "files": [
    "lib",
    "packages"
  ],
  "scripts": {
    "bootstrap": "yarn || npm i && cd ./packages/drips-css/ && yarn || npm i && cd ../../",
    "dev": "npm run build:entry && webpack-dev-server --inline --config build/webpack.config.dev.js --content-base ./",
    "build:entry": "node build/bin/build-entry.js",
    "build:components": "node build/bin/build-components.js --color",
    "build:drips-css": "gulp build --gulpfile packages/drips-css/gulpfile.js --color",
    "build:drips": "cross-env NODE_ENV=production webpack --progress --hide-modules --color --config build/webpack.build.js && cross-env NODE_ENV=production webpack -p --progress --hide-modules --color --config build/webpack.build.js",
    "build:style-entry": "cross-env VUE_ENV=server node build/bin/build-style-entry.js",
    "unit": "cross-env BABEL_ENV=test karma start test/karma.config.js --single-run",
    "test": "npm run unit",
    "dist": "node build/bin/build-lib.js",
    "lint": "./node_modules/.bin/eslint ./packages --ext .js,.vue"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:shuidi-fed/drips.git"
  },
  "keywords": [
    "shuidi",
    "vue",
    "component",
    "mobile",
    "ui"
  ],
  "author": "shuidi-fed",
  "license": "MIT",
  "peerDependencies": {
    "vue": ">= 2.5.0"
  },
  "devDependencies": {
    "@sd/vue-sfc-compiler": "^1.1.0",
    "@vue/test-utils": "^1.0.0-beta.13",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chalk": "^2.3.2",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "dependency-tree": "^5.12.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.4.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fast-vue-md-loader": "^1.0.3",
    "file-loader": "^1.1.11",
    "fs-extra": "^5.0.0",
    "hammer-touchemulator": "^0.0.2",
    "html-webpack-plugin": "^3.2.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.5.0",
    "karma-sinon-chai": "^1.3.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.0.5",
    "npm": "^5.8.0",
    "nprogress": "^0.2.0",
    "phantomjs-prebuilt": "^2.1.16",
    "postcss": "^6.0.21",
    "postcss-calc": "^6.0.1",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.1.3",
    "precss": "^2.0.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "shelljs": "^0.8.1",
    "sinon": "^4.5.0",
    "sinon-chai": "^2.12.0",
    "style-loader": "^0.19.1",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^1.0.1",
    "vue": "^2.5.16",
    "vue-easy-doc": "1.2.3",
    "vue-loader": "^14.2.2",
    "vue-router": "^3.0.1",
    "vue-style-loader": "^3.1.2",
    "vue-template-compiler": "^2.5.16",
    "vue-template-es2015-compiler": "^1.6.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.2",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {}
}
