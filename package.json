{
  "name": "drips",
  "version": "0.0.1",
  "description": "A Vue.js 2.0 Mobile UI at ShuiDi.",
  "main": "lib/drips.js",
  "style": "lib/drips-css/index.css",
  "files": [
    "lib",
    "packages"
  ],
  "scripts": {
    "bootstrap": "yarn || npm i && cd ./packages/drips-css/ && yarn || npm i && cd ../../",
    "dev": "npm run build:entry && webpack-dev-server --inline --config build/webpack.config.dev.js --content-base ./",
    "build:entry": "node build/bin/build-entry.js",
    "build:components": "node build/bin/build-components.js --color",
    "build:drips-css": "gulp build --gulpfile packages/drips-css/gulpfile.js --color",
    "build:drips": "cross-env NODE_ENV=production webpack --progress --hide-modules --color --config build/webpack.build.js && cross-env NODE_ENV=production webpack -p --progress --hide-modules --color --config build/webpack.build.js",
    "build:style-entry": "cross-env VUE_ENV=server node build/bin/build-style-entry.js",
    "dist": "node build/bin/build-lib.js",
    "lint": "./node_modules/.bin/eslint ./packages --ext .js,.vue"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:shuidi-fed/drips.git"
  },
  "keywords": [
    "shuidi",
    "vue",
    "component",
    "mobile",
    "ui"
  ],
  "author": "shuidi-fed",
  "license": "MIT",
  "peerDependencies": {
    "vue": ">= 2.5.0"
  },
  "devDependencies": {
    "@sd/vue-sfc-compiler": "^1.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cache-loader": "^1.2.0",
    "chalk": "^2.3.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.8",
    "dependency-tree": "^5.12.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fast-vue-md-loader": "^1.0.3",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "fs-extra": "^5.0.0",
    "hammer-touchemulator": "^0.0.2",
    "html-webpack-plugin": "^2.30.1",
    "npm": "^5.6.0",
    "nprogress": "^0.2.0",
    "postcss": "^6.0.16",
    "postcss-calc": "^6.0.1",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.0.10",
    "precss": "^2.0.0",
    "progress-bar-webpack-plugin": "^1.10.0",
    "shelljs": "^0.8.0",
    "style-loader": "^0.19.1",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^0.6.2",
    "vue": "^2.5.13",
    "vue-easy-doc": "1.2.3",
    "vue-loader": "^13.7.0",
    "vue-router": "^3.0.1",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.13",
    "vue-template-es2015-compiler": "^1.6.0",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-server": "^2.11.0"
  }
}
